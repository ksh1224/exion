{
  "name": "exion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "local": "dotenv -e ./dotenv/.env -- nodemon 'server' --exec 'ts-node' server -e ts,graphql",
    "dev": "dotenv -e ./dotenv/dev.env -- ts-node --transpile-only server",
    "start": "dotenv -e ./dotenv/prod.env ts-node server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "introspect": "npx prisma introspect",
    "generate": "npx prisma generate",
    "generate:nexus": "ts-node --transpile-only server/src/schema",
    "migrate:init": "rm -rf prisma/migrations && npx prisma migrate save --name init --experimental",
    "migrate:save": "dotenv -e ./dotenv/.env -- npx prisma migrate save --experimental",
    "migrate:up": "dotenv -e ./dotenv/.env -- npx prisma migrate up --experimental",
    "migrate:down": "dotenv -e ./dotenv/.env -- npx prisma migrate down 1 --experimental",
    "migrate:dev": "dotenv -e ./dotenv/dev.env -- npx prisma migrate up --experimental",
    "migrate:prod": "dotenv -e ./dotenv/prod.env -- npx prisma migrate up --experimental",
    "migrate:test": "dotenv -e ./dotenv/test.env -- npx prisma migrate up --experimental",
    "next:local": "dotenv -e ./dotenv/.env -- next dev",
    "next:dev": "dotenv -e ./dotenv/.env -- next dev",
    "next:start": "dotenv -e ./dotenv/dev.env -- next start",
    "next:build": "dotenv -e ./dotenv/.env -- next build"
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@date-io/moment": "1.x",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "graphql": "^15.3.0",
    "moment": "^2.28.0",
    "next": "9.5.3",
    "react": "^16.14.0",
    "react-dom": "16.13.1",
    "sass": "^1.27.0",
    "webpack": "^5.1.3",
    "@nexus/schema": "^0.16.0",
    "@prisma/client": "^2.9.0",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "eslint-plugin-graphql": "^4.0.0",
    "express": "^4.17.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "i18next": "^19.7.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-prisma": "^0.21.0",
    "node-rsa": "^1.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.1.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "@prisma/cli": "^2.8.1",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.7",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-relay": "^0.6.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-rsa": "^1.0.0",
    "graphql-import": "^1.0.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
